{
  "name": "vienna-ui",
  "version": "11.11.4",
  "description": "Raiffeisenbank Design System",
  "license": "MIT",
  "repository": {
    "type": "http",
    "url": ""
  },
  "publishConfig": {
    "registry": ""
  },
  "main": "dist/index.js",
  "module": "esm/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./esm ",
    "build": "npm run build:cjs && npm run build:esm && npm run build:ts",
    "build:cjs": "npx babel ./src --extensions \".ts,.tsx\" --out-dir dist --ignore src/**/*.mdx,src/**/*.spec.ts,src/**/*.spec.tsx,src/**/*.stories.tsx",
    "build:esm": "cross-env BABEL_ENV=esm npx babel ./src --extensions \".ts,.tsx\" --out-dir esm --ignore src/**/*.mdx,src/**/*.spec.ts,src/**/*.spec.tsx,src/**/*.spec.ts,src/**/*.stories.tsx",
    "build:ts": "tsc --emitDeclarationOnly --project ./ --rootDir ./src --declarationDir dist",
    "build:flow": "node ./scripts/flowgen.js",
    "watch": "concurrently \"npm run build:esm -- --watch\" \"npm run build:ts -- --watch\"",
    "storybook": "storybook dev -p 6006 NODE_OPTIONS=--openssl-legacy-provider",
    "build-storybook": "storybook build",
    "lint": "eslint ./src --quiet --ext .js,.jsx,.ts,.tsx --config ./.eslintrc.js --fix",
    "lint:css": "stylelint './src/**/*.styles.tsx'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prettier": "npx prettier --write **/*.{js,ts,tsx,md,mdx,yml}",
    "test": "jest --no-cache",
    "test:watch": "jest --no-cache --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "release": "standard-version",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "prepush": "npm run lint && npm run typecheck",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:chromium": "npx playwright test --project=chromium",
    "playwright:debug": "npx playwright test --debug",
    "playwright:codegen": "npx playwright codegen",
    "playwright:report": "npx playwright show-report",
    "outdated": "npm outdated --long --json | npm-outdated-html --output ./reports/outdated.html"
  },
  "dependencies": {
    "@floating-ui/react": "^0.24.6",
    "@popperjs/core": "2.10.2",
    "date-fns": "2.21.3",
    "element-closest-polyfill": "1.0.0",
    "imask": "7.1.3",
    "react-focus-lock": "2.9.4",
    "react-imask": "7.1.3",
    "react-number-format": "5.1.4",
    "react-popper": "2.3.0",
    "vienna.icons": "^4.0.4",
    "vienna.react-use": "^3.1.0",
    "vienna.tokens": "^3.9.1",
    "vienna.ui-primitives": "^3.2.0",
    "vienna.ui-theme": "^4.8.0",
    "vienna.utils": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@cfaester/enzyme-adapter-react-18": "0.6.0",
    "@commitlint/cli": "17.2.0",
    "@playwright/test": "^1.32.3",
    "@storybook/addon-actions": "7.0.6",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-links": "7.0.6",
    "@storybook/react": "7.0.6",
    "@storybook/react-webpack5": "^7.5.0",
    "@tanstack/react-table": "8.7.6",
    "@testing-library/dom": "8.19.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "axios": "^1.6.5",
    "babel-loader": "8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-styled-components": "2.1.4",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "css": "^3.0.0",
    "dotenv-webpack": "6.0.4",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-jsdoc": "39.6.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "flowgen": "1.16.1",
    "glob": "8.0.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "jest-bamboo-formatter": "1.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "16.0.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.1.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.11",
    "rimraf": "3.0.2",
    "standard-version": "9.5.0",
    "storybook": "^7.6.8",
    "styled-components": "^6.0.5",
    "stylelint": "15.6.0",
    "stylelint-config-recommended": "12.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.5.4",
    "vienna.babel-plugin-inline-import": "^1.3.2",
    "vienna.config-commitlint": "^4.0.0",
    "vienna.config-eslint": "^4.0.4",
    "vienna.config-prettier": "^4.0.1",
    "vienna.config-typescript": "^3.4.0",
    "webpack": "^5.89.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-middleware": "5.3.3",
    "webpack-dev-server": "4.11.1"
  },
  "peerDependencies": {
    "imask": "7.1.3",
    "react-number-format": "5.1.4",
    "styled-components": ">=6.0.0"
  },
  "overrides": {
    "postcss": "8.4.33"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run prettier",
      "npm run lint --",
      "npm run typecheck",
      "git add"
    ]
  },
  "bugs": {
